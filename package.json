    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "a-cdk": "^0.1.48",
    "a-conf": "^0.1.12",
    "a-uik": "^0.1.18",
    "aws-amplify": "^3.3.13",
    "aws-cdk": "^1.81.0",
    "aws-sdk": "^2.820.0",
    "constructs": "^3.2.90",
    "framer-motion": "^3.1.1",
    "graphql": "^15.3.0",
    "next": "^10.0.4",
    "next-seo": "^4.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-hook-form": "^6.14.0"
