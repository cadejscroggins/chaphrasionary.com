{
  "private": true,
  "name": "chaphrasionary.com",
  "scripts": {
    "bootstrap": "cdk bootstrap",
    "build": "next build",
    "deploy:dev": "ENV=dev cdk deploy --outputs-file cdk.outputs.json",
    "diff:dev": "ENV=dev cdk diff",
    "clean": "rm -rf node_modules cdk.out .next cdk.outputs.json",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,json,graphql,md}\"",
